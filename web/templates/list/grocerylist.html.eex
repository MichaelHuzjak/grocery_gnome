<div class="container">
  <h2>Grocery</h2>                                                                              
  <div class="table-responsive">          
  <table class="table table-hover table-condensed" id="pTable">
    <thead>
      <tr>
        <th id="Food Name">Food name</th>
        <th id="Food Amount">Food Amount</th>
        <th id="Exp Date">Exp. Date</th>
	<th id ="Move">Move to Pantry</th>
	<th></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td align="left" contenteditable="true">Milk</td>
        <td align="left" contenteditable="true">1 gal</td>
        <td align="center" contenteditable="true">3/21/2016</td>
	<td align="center"><input type="checkbox" name="name1" />&nbsp;</td>
	<td><input type="button" value="Delete" onclick="deleteRow(this)"/></td>
      </tr>
    </tbody>
  </table>
  </div>
  <button class="btn btn-success" onclick="addFood()">Add Food Item</button>
  <button class="btn btn-primary">Move selected items to Pantry</button>
</div>



